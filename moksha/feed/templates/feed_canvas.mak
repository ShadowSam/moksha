<div id="${id}" class="list">
    <h3><a href="${link}" target="_blank">${name}</a></h3>
    <table>
        % for entry in entries:
            <tr>
                <td id="${entry['uid']}">
                    <h4><a href="${entry.link}" target="_blank">${entry.title}</a></h4>
                    <div id="${entry['uid']}_content">
                        ${entry.content[0].value}
                    </div>
                </td>
            </tr>
        % endfor
    </table>
</div>
